    "dev": "next dev",
    "dev:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "check": "oxlint && tsc --noEmit",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "prepare": "node .husky/install.mjs || true",
    "db:up": "drizzle-kit up --config=drizzle-dev.config.ts",
    "db-prod:up": "drizzle-kit up --config=drizzle-prod.config.ts",
