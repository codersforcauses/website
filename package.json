    "db-prod:migrate": "drizzle-kit migrate  --config=drizzle-prod.config.ts",
    "db:push": "drizzle-kit push --config=drizzle-dev.config.ts",
    "db-prod:push": "drizzle-kit push  --config=drizzle-prod.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle-dev.config.ts",
    "db-prod:studio": "drizzle-kit studio  --config=drizzle-prod.config.ts",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:fix": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "email:dev": "email dev --dir=\"./src/emails\" --port=3030"
