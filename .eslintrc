{
  "extends": ["standard", "standard-react", "plugin:cypress/recommended"],
  "parser": "@typescript-eslint/parser",
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "no-unused-vars": "off",
        "no-undef": "off",
        "space-before-function-paren": "off",
        "camelcase": "off",
        "no-use-before-define": "off",
        "react/react-in-jsx-scope": "off"
      },
      "globals": {
        "React": "writable"
      }
    }
  ]
}
